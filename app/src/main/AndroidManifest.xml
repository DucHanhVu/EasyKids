<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quynhon.easykids">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainControl" />
        <activity android:name=".VideoPackage.VideoActivity" />
        <activity
            android:name=".VideoPackage.VideoView"
            android:screenOrientation="landscape" />
        <activity android:name=".GamePackage.GameActivity" />
        <activity android:name=".MusicPackage.MusicActivity"></activity>
    </application>

</manifest>